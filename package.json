{
  "name": "team-manage-app",
  "private": false,
  "version": "1.0.0",
  "type": "module",
  "description": "Dashboard do analizy KPI zespołu deweloperskiego z integracją z Jira",
  "repository": {
    "type": "git",
    "url": "https://github.com/BartoszBossy-pixel/team-manage-app.git"
  },
  "author": "Bartosz Bossy",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "server": "node server.js",
    "start": "concurrently \"npm run server\" \"npm run dev\"",
    "start:dev": "concurrently --names \"PROXY,CLIENT\" --prefix-colors \"blue,green\" \"npm run server\" \"npm run dev\"",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "init-db": "node scripts/init-dynamodb-local.js",
    "seed-pixels": "node scripts/seed-pixels-team.js",
    "start:with-db": "npm run docker:up && sleep 3 && npm run init-db && npm run start"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.975.0",
    "@aws-sdk/lib-dynamodb": "^3.975.0",
    "aws-sdk": "^2.1693.0",
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "nodemon": "^3.1.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.13.0",
    "recharts": "^2.8.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "sass": "^1.97.3",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
